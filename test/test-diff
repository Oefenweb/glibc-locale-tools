#!/usr/bin/env bash
#
# set -x;
set -e;
set -o pipefail;
#
thisFile="$(readlink -f ${0})";
thisFilePath="$(dirname ${thisFile})";

locale="${1}"

.././locale-decode < data/${locale} > data/${locale}.decoded;
.././locale-encode < data/${locale}.decoded > data/${locale}.encoded;

diff -u data/${locale} data/${locale}.encoded;
